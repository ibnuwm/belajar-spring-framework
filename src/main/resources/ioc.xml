<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="ibnu" class="com.ibnuwm.entity.Person">
        <property name="nama" value="Ibnu Wanhar M"></property>
        <property name="alamat" ref="serang"></property>
    </bean>

    <bean id="serang" class="com.ibnuwm.entity.Address">
        <property name="jalan" value="Jl. Saleh Baimin"></property>
        <property name="kota" value="Serang"></property>
        <property name="kodepos" value="42111"></property>
    </bean>
</beans>